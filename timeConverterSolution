function timeConverter(str24) {
var strHr = str24.substr(0,2);
var numHr = parseInt(strHr, 10);
var strMin = str24.substr(3,6);
var numMin = parseInt(strMin, 10);
var amPm


if (numHr > 12){
var amPm "PM"
var numHr -= 12
return numHr
return amPm;
} else { 
var amPm = "AM"
return amPm;
}

var convertedTime = numHr + ":" + numMin + amPm


return convertedTime;
}

